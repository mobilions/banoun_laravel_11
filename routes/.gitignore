# Laravel .gitignore

# Dependencies
/node_modules
/vendor
/composer.lock
/package-lock.json
/yarn.lock

# Environment files
.env
.env.backup
.env.production
.env.local
.env.testing

# Storage files (except structure)
/storage/*.key
/storage/framework/cache/data/
/storage/framework/sessions/
/storage/framework/views/
/storage/logs/
/storage/debugbar/

# Public storage (if using local disk)
/public/storage

# Cache files
/bootstrap/cache/

